package backend;
import java.util.Comparator;

public class CompatatorID implements Comparator<Student>{

	
	@Override
	public int compare(Student s1, Student s2) {
		if(s1 == null){
			return -1;
		}
		if(s2 == null){
			return 1;
		}
		if(s1.getId() < s2.getId()){
			return -1;
		} else if(s1.getId() > s2.getId()){
			return 1;
		}
		return 0;
	}
	

}
